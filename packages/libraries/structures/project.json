{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "name": "libraries-structures",
  "projectType": "library",
  "sourceRoot": "packages/libraries/structures/src",
  "tags": [ ],
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "options": {
        "assets": [
          "packages/libraries/structures/*.md"
        ],
        "main": "packages/libraries/structures/src/index.ts",
        "outputPath": "dist/packages/libraries/structures",
        "tsConfig": "packages/libraries/structures/tsconfig.lib.json"
      },
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "packages/libraries/structures/**/*.ts"
        ]
      },
      "outputs": [
        "{options.outputFile}"
      ]
    },
    "test": {
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      },
      "executor": "@nrwl/jest:jest",
      "options": {
        "jestConfig": "packages/libraries/structures/jest.config.ts",
        "passWithNoTests": true
      },
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ]
    }
  }
}
