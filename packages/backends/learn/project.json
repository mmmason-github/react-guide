{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "name": "backends-learn",
  "projectType": "application",
  "sourceRoot": "packages/backends/learn/src",
  "tags": [ ],
  "targets": {
    "build": {
      "configurations": {
        "development": { },
        "production": {
          "fileReplacements": [
            {
              "replace": "packages/backends/learn/src/environments/environment.dev.ts",
              "with": "packages/backends/learn/src/environments/environment.prod.ts"
            }
          ]
        }
      },
      "defaultConfiguration": "production",
      "executor": "@nrwl/webpack:webpack",
      "options": {
        "assets": [
          "packages/backends/learn/src/docs/dist"
        ],
        "compiler": "tsc",
        "isolatedConfig": true,
        "main": "packages/backends/learn/src/main.ts",
        "outputPath": "dist/packages/backends/learn",
        "target": "node",
        "tsConfig": "packages/backends/learn/tsconfig.app.json",
        "webpackConfig": "packages/backends/learn/webpack.config.js"
      },
      "outputs": [
        "{options.outputPath}"
      ]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "packages/backends/learn/**/*.ts"
        ]
      },
      "outputs": [
        "{options.outputFile}"
      ]
    },
    "serve": {
      "configurations": {
        "development": {
          "buildTarget": "backends-learn:build:development"
        },
        "production": {
          "buildTarget": "backends-learn:build:production"
        }
      },
      "defaultConfiguration": "development",
      "executor": "@nrwl/js:node",
      "options": {
        "buildTarget": "backends-learn:build"
      }
    },
    "test": {
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      },
      "executor": "@nrwl/jest:jest",
      "options": {
        "jestConfig": "packages/backends/learn/jest.config.ts",
        "passWithNoTests": true
      },
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ]
    }
  }
}
